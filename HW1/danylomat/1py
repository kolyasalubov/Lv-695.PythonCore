name = input("What is your name?")
age= input("How old are you?")
city= input("Where do you live?")
print("Hello,"+name)
print("Your age is "+age)
print("You live in "+ city)

a=int(input("a: "))
b=int(input("b: "))
print("a+b=", a+b)
print("a-b=", a-b)
print("a*b=", a*b)
print("a/b=", a/b)
print("a**b=", a**b)