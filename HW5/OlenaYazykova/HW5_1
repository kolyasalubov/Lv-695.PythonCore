even_numb=[]
odd_div_by_3=[]
not_div_by_2and3=[]
for item in range(1,11):
    if item%2==0:
        even_numb.append(item)
    elif item%3==0:
        odd_div_by_3.append(item)
    else:
        not_div_by_2and3.append(item)
print(f"Even numbers are: {even_numb}")
print(f"Odd n, umbers that are divisible by 3: {odd_div_by_3}")
print(f"Numbers that are not divisible by 2 and 3: {not_div_by_2and3}")
