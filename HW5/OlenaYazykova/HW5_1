my_list=list(range(1,11))
even_numb=[]
div_by_3=[]
not_div_by_2and3=[]
for i in range(len(my_list)):
    if my_list[i]%2==0:
        even_numb.append(my_list[i])
    if my_list[i]%3==0:
        div_by_3.append(my_list[i])
    if my_list[i]%2!=0 and my_list[i]%3!=0:
        not_div_by_2and3.append(my_list[i])
print(f"Even numbers are: {even_numb}")
print(f"Numbers that are divisible by 3: {div_by_3}")
print(f"Numbers that are not divisible by 2 and 3: {not_div_by_2and3}")