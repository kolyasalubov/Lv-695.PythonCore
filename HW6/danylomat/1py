first_number=float(input("Enter the first number:"))
second_number=float(input("Enter the second number:"))
def largest_number(first_number, second_number):
    if first_number>second_number:
        return f"The largest number is {first_number}."
    elif first_number<second_number:
        return f"The largest number is {second_number}."
    else:
        return f"These numbers are equal: {first_number} = {second_number}"
print(largest_number(first_number, second_number))
print(largest_number.__doc__)